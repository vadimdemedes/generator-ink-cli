{
	"name": "<%= moduleName %>",
	"version": "0.0.0",
	"description": "<%= moduleDescription %>",
	"license": "MIT",
	"repository": "<%= githubUsername %>/<%= repoName %>",
	"author": {
		"name": "<%= name %>",
		"email": "<%= email %>",
		"url": "<%= humanizedWebsite %>"
	},
	"bin": "cli.js",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"test": "xo && <% if (nyc) { %>nyc <% } %>ava"
	},
	"files": [
		"cli.js",
		"ui.js"
	],
	"keywords": [
		""
	],
	"dependencies": {
		"ink": "^0.5.0",
		"meow": "^5.0.0",
		"prop-types": "^15.6.1",
		"import-jsx": "^1.3.0"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",<% if (codecov) { %>
		"codecov": "^3.0.1",<% } %>
		"eslint-config-xo-react": "^0.16.0",
		"eslint-plugin-react": "^7.7.0",<% if (nyc) { %>
		"nyc": "^11.7.1",<% } %>
		"xo": "0.20.3"
	},
	"ava": {
		"babel": {
			"presets": [
				"@ava/stage-4"
			],
			"plugins": [
				[
					"transform-react-jsx",
					{
						"pragma": "h",
						"useBuiltIns": true
					}
				]
			]
		}
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"rules": {
			"react/no-unused-prop-types": 1
		},
		"settings": {
			"react": {
				"pragma": "h"
			}
		}
	}<% if (codecov) { %>,
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	}<% } %>
}
